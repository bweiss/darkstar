config bool AUTO_RESPONSE 0
config bool AUTO_RESPONSE_BEEP 0
config      AUTO_RESPONSE_STR 
config      BANTYPE 3
config      DEFAULT_KICK_REASON No soup for you!
config      MAX_NICK_LENGTH 9
config bool SHOW_OPVOICE_IN_PUBLICS 0

format BANS $[3]1 $[12]5  $strftime($6 %x@%X)  $4
format BANS_FOOTER $G Total bans on $3: $2
format BANS_HEADER #   SetBy         When               Ban
format CHANNEL_SIGNOFF $G SignOff $1: $2 \($3-%n\) [$tolower($Z)]
format CHANNEL_SYNC $G Channel $1 synced in $2 seconds
format CHANNEL_TS $G Channel $2 created $strftime($3 %a %Ef %T %Z %Y) [$3]
format JOIN $G $1 \($2\) has joined $3 at $Z
format LEAVE $G $1 \($2\) has left $3 at $Z
format MODE $G Mode change \"$3-\" on channel $2 by $1
format PUBLIC <$1> $3-
format PUBLIC_AR <%W$1%n> $3-
format PUBLIC_OTHER <$1:$2> $3-
format PUBLIC_OTHER_AR <%W$1%n:$2> $3-
format SEND_PUBLIC <$2> $3-
format SEND_PUBLIC_OTHER <$1:$2> $3-
format TOPIC $G Topic for $2: $3-
format TOPIC_CHANGE $G $1 has changed the topic on channel $2 to $3-
format TOPIC_SETBY $G The topic was set by $3 on $getstrftime($4)
