queue cleanup.debug
{
	^bind -symbolic key_f10 NOTHING;
};

bind -symbolic key_f10 PARSE_COMMAND debug.toggle;
	
alias debug.toggle (void)
{
	if (DEBUG) {
		set DEBUG 0;
	} else {
		set DEBUG 7;
	};
};

